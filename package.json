{
  "name": "some-utils-three",
  "version": "1.0.0",
  "description": "A library for Three.js utilities and components",
  "main": "",
  "scripts": {
    "dev": "tsc-watch --onSuccess \"node scripts/post-build.mjs\"",
    "build": "rm -rf dist && tsc && node scripts/post-build.mjs"
  },
  "keywords": [
    "three.js",
    "typescript",
    "3d",
    "graphics",
    "webgl"
  ],
  "author": "jniac",
  "license": "ISC",
  "peerDependencies": {
    "some-utils-dom": "workspace:*",
    "some-utils-ts": "workspace:*",
    "three": ">=0.172.0"
  },
  "devDependencies": {
    "@types/three": ">=0.172.0",
    "three": ">=0.172.0",
    "tsc-watch": "^6.2.1",
    "typescript": "^5.5.4"
  },
  "exports": {
    "./ts/*": {
      "default": "./src/*.ts"
    },
    "./*": {
      "default": "./dist/*.js",
      "types": "./dist/*.d.ts"
    }
  },
  "files": [
    "dist",
    "src"
  ]
}